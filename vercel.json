{
    "version": 2,
    "name": "powertech-api",
    "public": true,
    "builds": [
       { "src": "src/index.js", "use": "@vercel/node" }
    ],
  "rewrites": [
    { "source": "/(.*)", "destination": "src/index.js" }
  ],
    "routes": [
       { "src": "/(.*)", "dest": "src/index.js" }
    ]
 }